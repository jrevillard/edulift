{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "exactOptionalPropertyTypes": false
  },
  "include": ["src/**/*", "src/**/__tests__/**/*", "**/*.test.ts", "**/*.spec.ts", "tests/**/*"],
  "exclude": ["node_modules", "dist"]
}